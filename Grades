#include<stdio.h>
void main()
{
int marks;
printf("\n Enter marks");
scanf("%d",&marks);
if(marks>=85&&marks<=100)
printf("\n Grade A");
else if(marks>=75&&marks<=84)
printf("\n Grade B");
else if(marks>=55&&marks<=69)
printf("\n Grade C");
else if( marks>=40&&marks<=54)
printf("\n Grade D");
else(marks<40)
printf("/n Grade F");
}
